end

function drw_hp()
	local t_hp,dis=hp,0
	for i=1,10 do
		print("‚ñà",126-8*i,3,13)
	end
	while(t_hp>0) do
		if t_hp-10>=0 then
			dis-=8
			print("‚ñà",126+dis,3,8)
			t_hp-=10
		else
			if (t_hp-2<=0) break
			if dis%2==0 then
				dis-=3
			else
				dis-=2
			end
			print("|",126+dis,3,8)
			t_hp-=2
		end
	end
end

function drw_mana()
	local t_mana,dis=mana,0
	while(t_mana>0) do
		dis-=6
		print("‚óè",124+dis,9,12)
		pset(128+dis,10,1)
		t_mana-=1
	end
end

